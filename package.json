{
  "name": "hanasho-next",
  "version": "1.0.0",
  "private": "true",
  "license": "UNLICENSED",
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "dev": "next dev",
    "fmtpackagejson": "sort-package-json 'package.json' 'apps/*/package.json' 'packages/*/package.json'",
    "fmtpackagejsoncheck": "yarn fmtpackagejson --check",
    "fmtprettier": "yarn fmtprettierbase --write",
    "fmtprettierbase": "prettier '**/*.{js,ts,tsx,json,md,html,yml}'",
    "fmtprettiercheck": "yarn fmtprettierbase --check",
    "format": "yarn fmtprettier && yarn fmtpackagejson",
    "formatcheck": "yarn fmtprettiercheck && yarn fmtpackagejsoncheck",
    "lint": "eslint . --ext .jsx,.js,.tsx,.ts --ignore-path .gitignore --cache",
    "lintfix": "yarn lint --fix",
    "markuplint": "markuplint --config .markuplintrc.ts 'apps/**/*.{html,tsx}' 'packages/**/*.{html,tsx}'",
    "prepare": "husky install",
    "scaffdog": "scaffdog generate",
    "start": "next start",
    "storybook": "storybook dev -p 6006"
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,md,html,yml}": [
      "yarn fmtprettier"
    ],
    "package.json": [
      "yarn fmtpackagejson"
    ],
    "*.{.jsx,.js,.tsx,.ts}": [
      "yarn lintfix"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^4.29.12",
    "lodash-es": "^4.17.21",
    "next": "^13.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-redux": "^8.0.7",
    "redux-persist": "^6.0.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@markuplint/jsx-parser": "^3.7.0",
    "@markuplint/react-spec": "^3.8.0",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/blocks": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/react-webpack5": "^7.0.18",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "css-loader": "^6.8.1",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-react": "^1",
    "eslint-config-react-app": "^6",
    "eslint-plugin-flowtype": "^8",
    "eslint-plugin-jest": "^27",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-local-rules": "^1.3.2",
    "eslint-plugin-promise": "^6",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^4",
    "eslint-plugin-simple-import-sort": "^10",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-testing-library": "^5",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "markuplint": "^3.10.0",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.1",
    "scaffdog": "^2.5.0",
    "sort-package-json": "^2",
    "storybook": "^7.0.18",
    "style-loader": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": "18.14.1"
  },
  "volta": {
    "node": "18.14.1",
    "yarn": "1.22.19"
  }
}
